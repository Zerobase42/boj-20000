def row(i, last_row: str) -> bool:
    b = ""
    for c in last_row:
        if '0' <= c <= '9':
            x = ord(c) - ord('0')
        else:
            x = ord(c) - ord('A') + 10
        # 4자리 2진수로 변환 후 이어붙임
        b += f"{x:04b}"
    return b[i] != '1'   # '1'이면 False, 아니면 True

def val_to_char(v: int) -> str:
    """0~9 → '0'-'9', 10~31 → 'A'-'V'"""
    if v < 10:
        return str(v)
    return chr(ord('A') + v - 10)

def char_to_val(c: str) -> int:
    """'0'-'9' → 0~9, 'A'-'V' → 10~31"""
    if '0' <= c <= '9':
        return int(c)
    return ord(c) - ord('A') + 10

def dec_to_base32(n: int) -> str:
    """10진수 → 32진수 문자열"""
    if n == 0:
        return "0"
    res = ""
    while n > 0:
        res = val_to_char(n % 32) + res
        n //= 32
    return res

def base32_to_dec(s: str) -> int:
    """32진수 문자열 → 10진수"""
    num = 0
    for c in s:
        num = num * 32 + char_to_val(c)
    return num

